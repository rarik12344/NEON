{
  "version": 2,
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/$1",
      "headers": {
        "Content-Type": "text/html"
      }
    },
    {
      "src": "/.well-known/farcaster.json",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=3600",
        "Content-Type": "application/json"
      }
    }
  ]
}

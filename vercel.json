{
  "version": 2,
  "routes": [
    {
      "src": "/api/frame",
      "dest": "/api/frame.js",
      "methods": ["GET", "POST"],
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Cache-Control": "public, max-age=3600"
      }
    },
    {
      "src": "/.well-known/farcaster.json",
      "headers": {
        "Cache-Control": "public, max-age=86400",
        "Content-Type": "application/json"
      }
    }
  ]
}

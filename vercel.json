{
  "version": 3,
  "build": {
    "env": {
      "NO_CACHE": "1"
    }
  },
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/api/farcaster.js",
      "headers": {
        "Cache-Control": "no-store, max-age=0",
        "CDN-Cache-Control": "no-store",
        "Vercel-CDN-Cache-Control": "no-store"
      }
    }
  ]
}

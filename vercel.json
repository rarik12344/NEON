{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/.well-known/farcaster.json",
      "headers": {
        "Cache-Control": "no-store, max-age=0"
      }
    },
    {
      "src": "/api/frame",
      "dest": "/api/frame.js",
      "methods": ["GET", "POST"]
    },
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.js",
      "methods": ["POST"]
    }
  ]
}

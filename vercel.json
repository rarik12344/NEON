{
  "version": 2,
  "headers": [
    {
      "source": "/.well-known/farcaster.json",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Cache-Control", "value": "no-store, max-age=0" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/api/frame", "destination": "/api/frame.js" },
    { "source": "/api/webhook", "destination": "/api/webhook.js" }
  ]
}

{
  "version": 2,
  "routes": [
    {
      "src": "/api/frame",
      "headers": {
        "Content-Type": "text/html",
        "Cache-Control": "no-store"
      }
    },
    {
      "src": "/.well-known/farcaster.json",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/(.*)",
      "headers": {
        "Content-Security-Policy": "default-src 'self'; img-src https://i.ibb.co; frame-src 'self' https://warpcast.com"
      }
    }
  ]
}

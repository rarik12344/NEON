{
  "version": 2,
  "routes": [
    {
      "src": "/api/frame",
      "headers": {
        "Content-Type": "text/html",
        "Cache-Control": "no-cache, no-store, must-revalidate"
      }
    },
    {
      "src": "/.well-known/farcaster.json",
      "headers": {
        "Content-Type": "application/json",
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}

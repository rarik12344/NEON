{
  "version": 2,
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/api/farcaster-manifest.js",
      "headers": {
        "Cache-Control": "no-store, max-age=0",
        "CDN-Cache-Control": "no-store"
      }
    },
    {
      "src": "/api/frame",
      "dest": "/api/frame.js",
      "methods": ["GET", "POST"]
    }
  ]
}
